# 1 "arch/arm64/boot/dts/seco/overlays/seco-imx8qm-c26-ov5640-csi1-overlay.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm64/boot/dts/seco/overlays/seco-imx8qm-c26-ov5640-csi1-overlay.dts"
# 18 "arch/arm64/boot/dts/seco/overlays/seco-imx8qm-c26-ov5640-csi1-overlay.dts"
/dts-v1/;
/plugin/;

# 1 "./scripts/dtc/include-prefixes/dt-bindings/clock/imx8qm-clock.h" 1
# 22 "arch/arm64/boot/dts/seco/overlays/seco-imx8qm-c26-ov5640-csi1-overlay.dts" 2
# 1 "./scripts/dtc/include-prefixes/dt-bindings/pinctrl/pads-imx8qm.h" 1
# 23 "arch/arm64/boot/dts/seco/overlays/seco-imx8qm-c26-ov5640-csi1-overlay.dts" 2
# 1 "./scripts/dtc/include-prefixes/dt-bindings/gpio/gpio.h" 1
# 24 "arch/arm64/boot/dts/seco/overlays/seco-imx8qm-c26-ov5640-csi1-overlay.dts" 2

/ {
        compatible = "fsl,imx8qm-mek","fsl,seco-imx8qm-c26", "fsl,imx8qm";
# 35 "arch/arm64/boot/dts/seco/overlays/seco-imx8qm-c26-ov5640-csi1-overlay.dts"
 fragment@0 {
                target = <&isi_4>;
                __overlay__ {
   status = "okay";
  };
 };

 fragment@2 {
                target = <&gpio0_mipi_csi1>;
                __overlay__ {
   status = "okay";
  };
 };

 fragment@3 {
                target = <&i2c0_mipi_csi1>;
                __overlay__ {
   #address-cells = <1>;
   #size-cells = <0>;
   status = "okay";
   ov5640_mipi: ov5640_mipi@3c {
    compatible = "ovti,ov5640_mipi_v3";
    reg = <0x3c>;
    pinctrl-names = "default";
    pinctrl-0 = <&pinctrl_mipi_csi1_camera>;
    clocks = <&clk 0>;
    clock-names = "csi_mclk";
    csi_id = <1>;
    pwn-gpios = <&gpio1 31 1>;
    rst-gpios = <&gpio1 30 0>;
    mclk = <24000000>;
    mclk_source = <0>;
    mipi_csi;
    status = "okay";
    port {
     ov5640_mipi_ep: endpoint {
      remote-endpoint = <&mipi_csi1_ep>;
     };
    };
   };
  };
 };

 fragment@4 {
                target = <&mipi_csi_1>;
                __overlay__ {
          status = "okay";
   port@1 {
    mipi_csi1_ep: endpoint {
     remote-endpoint = <&ov5640_mipi_ep>;
     data-lanes = <1 2>;
    };
   };
  };
 };

 fragment@5 {
                target = <&gpio2>;
                __overlay__ {
   status = "okay";
      line_b@0 {
   gpio-hog;
   gpios = <25 0>;
   output-high;
   line-name = "HDMI_SEL1";
   };

      line_b@1 {
   gpio-hog;
   gpios = <26 0>;
   output-high;
   line-name = "HDMI_SEL2";
   };

      line_b@2 {
   gpio-hog;
   gpios = <27 0>;
   output-high;
   line-name = "HDMI_IN_EN";
   };

  };
 };
};
