/ {
        compatible = "fsl,imx8qm-mek","fsl,seco-imx8qm-c43", "fsl,imx8qm";
# 35 "arch/arm64/boot/dts/seco/overlays/seco-imx8qm-c43-ov5640-csi0-overlay.dts"
 fragment@0 {
                target = <&isi_0>;
                __overlay__ {
   status = "okay";
  };
 };

 fragment@2 {
                target = <&gpio0_mipi_csi0>;
                __overlay__ {
   status = "okay";
  };
 };

 fragment@3 {
                target = <&i2c0_mipi_csi0>;
                __overlay__ {
   #address-cells = <1>;
   #size-cells = <0>;
   status = "okay";
   ov5640_mipi: ov5640_mipi@3c {
    compatible = "ovti,ov5640_mipi_v3";
    reg = <0x3c>;
    pinctrl-names = "default";
    pinctrl-0 = <&pinctrl_mipi_csi0_camera>;
    clocks = <&clk 0>;
    clock-names = "csi_mclk";
    csi_id = <0>;
    pwn-gpios = <&gpio1 28 1>;
    rst-gpios = <&gpio1 27 0>;
    mclk = <24000000>;
    mclk_source = <0>;
    mipi_csi;
    status = "okay";
    port {
     ov5640_mipi_ep: endpoint {
      remote-endpoint = <&mipi_csi0_ep>;
      data-lanes = <1 2>;
     };
    };
   };
  };
 };

 fragment@4 {
                target = <&mipi_csi_0>;
                __overlay__ {
          status = "okay";
   port@0 {
    mipi_csi0_ep: endpoint {
     remote-endpoint = <&ov5640_mipi_ep>;
     data-lanes = <1 2>;
    };
   };
  };
 };
};
