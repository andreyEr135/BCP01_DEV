 };

 uart2: serial@30890000 {
  compatible = "fsl,imx8mm-uart",
        "fsl,imx6q-uart", "fsl,imx21-uart";
  reg = <0x0 0x30890000 0x0 0x10000>;
  interrupts = <0 27 4>;
  clocks = <&clk 418>,
   <&clk 418>;
  clock-names = "ipg", "per";
