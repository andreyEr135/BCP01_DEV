  reg = <0x0 0x30880000 0x0 0x10000>;
  interrupts = <0 28 4>;
  clocks = <&clk 419>,
   <&clk 419>;
  clock-names = "ipg", "per";
  dmas = <&sdma1 26 4 0>, <&sdma1 27 4 0>;
  dma-names = "rx", "tx";
