        "fsl,imx6q-uart", "fsl,imx21-uart";
  reg = <0x0 0x30860000 0x0 0x10000>;
  interrupts = <0 26 4>;
  clocks = <&clk 417>,
   <&clk 417>;
  clock-names = "ipg", "per";
  dmas = <&sdma1 22 4 0>, <&sdma1 23 4 0>;
